{
  "name": "ldc",
  "version": "0.0.7",
  "description": "Audiovisual framework for Loop.Coop",
  "main": "build/ldc.js",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "usage"
  },
  "devDependencies": {
    "apage": "latest",
    "coffee-script": "latest",
    "nodemon": "latest"
  },
  "dependencies": {
  },
  "keywords": [
    "CoffeeScript",
    "Web Audio",
    "Loop.Coop",
    "Sequencer",
    "Synthesizer",
    "Visualizer",
    "Animation"
  ],
  "scripts": {
    "hi": "open .; open index.html; open test/run-test.html; subl .",
    "make": "node bin/akaybe-build",
    "index": "node_modules/apage/bin/apage -i README.md doc/*.md -o index.html -p \"`cat node_modules/apage/build/plugin/*.js`\"",
    "test": "node test/run-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richplastow/ldc.git"
  },
  "author": "Rich Plastow <rich@richplastow.com> (http://richplastow.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richplastow/ldc/issues"
  },
  "homepage": "http://ldc.richplastow.com/"
}
